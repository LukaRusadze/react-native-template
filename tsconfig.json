{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@components": ["./components"],
      "@components/*": ["./components/*"],
      "@navigation": ["./navigation"],
      "@navigation/*": ["./navigation/*"],
      "@primitives": ["./primitives"],
      "@primitives/*": ["./primitives/*"],
      "@store": ["./store"],
      "@store/*": ["./store/*"],
      "@screens": ["./screens"],
      "@screens/*": ["./screens/*"]
    }
  }
}
