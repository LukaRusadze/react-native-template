{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@navigation": "./src/navigation",
          "@primitives": "./src/primitives",
          "@screens": "./src/screens",
          "@store": "./src/store",
          "@hooks": "./src/hooks",
          "@assets": "./assets",
          "@constants": "./src/constants"
        }
      }
    ]
  ]
}
